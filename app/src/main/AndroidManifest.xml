<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cookandroid.finalproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FinalProject">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AttendanceActivity" android:label="Attendance 액티비티"/>
        <activity android:name=".CalendarActivity" android:label="Calendar 액티비티"/>
        <activity android:name=".PhotoActivity" android:label="Photo 액티비티"/>
        <activity android:name=".AnnounceActivity" android:label="Announce 액티비티"/>
    </application>

</manifest>